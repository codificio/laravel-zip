{
    "name": "codificio/laravel-zip",
    "description": "A small utility to handle a zip file in Laravel",
    "keywords": ["zip", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Enrico Nardo",
            "email": "enrico@codificio.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelZip\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelZip\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
